{   
    "rulesDirectory": ["./node_modules/tslint-consistent-codestyle"],
	"rules": {
        "indent": [true, "spaces"],
        "no-unused-variable": true,
        "no-duplicate-variable": true,
		"triple-equals": [true, "allow-null-check"],
        "variable-name": [true, "ban-keywords"],
        "no-any": true
    },
    "include": [
        "./src/**/*.ts"
    ]
}